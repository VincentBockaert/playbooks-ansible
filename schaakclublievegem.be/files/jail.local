[sshd]
findtime=600
maxretry=5
bantime=86400
enabled = true

[apache]
enabled = true
port = http, https
filter = apache-auth
logpath = /var/log/apache*/*error.log
maxretry = 6

[apache-noscript]
enabled = true

[php-url-fopen]
enabled = true
port    = http,https
filter  = php-url-fopen
logpath = /var/log/apache*/*access.log